{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "deleteOutDir": true,
        "builder": {
            "type": "swc",
            "options": {
                "swcrcPath": ".build.swcrc"
            }
        },
        "typeCheck": true
    }
}
